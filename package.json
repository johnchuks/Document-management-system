{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "managing documents, roles and privledges",
  "main": "server/config/server.js",
  "scripts": {
    "test": " npm run test:server && npm run test:client",
    "test:server": "npm run test-build && NODE_ENV=test PORT=4000 nyc --reporter=text mocha --timeout 10000 --compilers js:babel-register -- ./server/tests/**/*.spec.js",
    "start-dev": "npm run build:dev",
    "test:client": "NODE_ENV=test nyc -e .jsx -e es6 mocha --compilers js:babel-core/register ./client/app/tests/helpers/browser.js ./client/app/tests/**/* --timeout 10000",
    "coverage": "nyc report --reporter=text-lcov  | coveralls",
    "build:dev": "webpack -w & babel-node -- server/config/server.js & nodemon server/config/server.js",
    "rebuild:testdb": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "seed:testdb": "NODE_ENV=test sequelize db:seed:all",
    "test-build": "npm run rebuild:testdb && npm run seed:testdb",
    "e2e": "nightwatch",
    "build": "webpack --config webpack.config.prod.js",
    "postinstall": "npm run build",
    "start": "npm run postinstall & babel-node -- server/config/server.js & nodemon server/config/server.js"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-johia/Document-management-system.git"
